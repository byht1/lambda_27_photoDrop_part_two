{
  "getAllAlbums": {
    "allOf": [
      { "$ref": "#/components/schemas/maxPage" },
      {
        "type": "object",
        "properties": {
          "albums": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "albumID": {
                  "type": "string",
                  "example": "b6aa1eff-6f05-4531-ab9c-12c63db20279"
                },
                "owner": {
                  "type": "string",
                  "example": "b6aa1eff-6f05-4531-ab9c-12c63db20279"
                },
                "name": {
                  "type": "string",
                  "example": "TestAlbum200"
                },
                "location": {
                  "type": "string",
                  "example": "Brooklyn"
                },
                "isPaid": {
                  "type": "boolean",
                  "example": true
                },
                "createdAt": {
                  "type": "string",
                  "example": "12/12/1955"
                }
              }
            }
          }
        }
      },
      { "$ref": "#/components/schemas/user" }
    ]
  }
}
